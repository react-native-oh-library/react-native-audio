{
  module: {
    name: 'audio',
    type: 'har',
    deviceTypes: ['default'],
    mainElement: "AudioRecorder",
    requestPermissions: [
      {
        name: 'ohos.permission.MICROPHONE',
        reason: "$string:label_permission_microphone",
        usedScene: {
          abilities: [
            "EntryAbility"
          ],
        },
      },
      {
        name: 'ohos.permission.WRITE_MEDIA',
        reason: "$string:label_permission_write_media",
        usedScene: {
          abilities: [
            "EntryAbility"
          ],
        },
      },
      {
        name: 'ohos.permission.READ_MEDIA',
        reason: "$string:label_permission_read_media",
        usedScene: {
          abilities: [
            "EntryAbility"
          ],
        },
      }
    ]
  },
}
